{
  "name": "nextt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:tsc": "tsc",
    "start": "next start",
    "dev:translations": "i18nexus pull && next dev",
    "build:translations": "i18nexus pull && next build",
    "start:translations": "i18nexus pull && next start",
    "lint": "next lint",
    "lintw": "next lint --fix",
    "prettier": "prettier src/**/*.{ts,tsx,js,css,html}",
    "prettierw": "prettier --write src/**/*.{ts,tsx,js,css,html}",
    "prepare": "husky"
  },
  "dependencies": {
    "html-react-parser": "^5.2.2",
    "husky": "^9.1.7",
    "i18next": "^24.1.2",
    "i18next-resources-to-backend": "^1.2.1",
    "lint-staged": "^15.4.1",
    "next": "^15.1.0",
    "next-auth": "^4.24.11",
    "next-i18n-router": "^5.5.1",
    "next-i18next": "^15.4.1",
    "next-video": "^2.0.1",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.2.0",
    "swiper": "^12.0.3"
  },
  "devDependencies": {
    "@types/i18next": "^12.1.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20",
    "@types/pdfmake": "^0.2.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint --fix \"src/**/*.{ts,tsx,json}\"",
      "prettier --write src/**/*.{ts,tsx,js,css,html}"
    ]
  }
}
